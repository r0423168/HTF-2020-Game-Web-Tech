{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/"
    ],
    "watch": [
        "./"
    ],
    "execMap": {
        "ts": "node -r ts-node/register"
    },
    "env": {
        "TS_NODE_PROJECT": "./tsconfig.json"
    },
    "ext": "js,json,ts,html"
}